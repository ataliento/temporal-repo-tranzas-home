{
  "name": "tranzas-home",
  "version": "1.0.0",
  "private": true,
  "main": "./src",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "test": "jest",
    "test:clean-cache": "jest --clearCache",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll  --runInBand",
    "test:watch:coverage": "jest --coverage --watchAll  --runInBand",
    "test:specific": "jest --watch -i --runInBand",
    "test:specific:coverage": "jest --watch -i --coverage",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
    "stylelint:fix": "stylelint --fix **/*.scss",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "docker:build": "docker build -t tranzas-home . ",
    "docker:start": "docker run -p 3001:9000 --env-file ./.env tranzas-home"
  },
  "dependencies": {
    "@module-federation/nextjs-mf": "6.1.1",
    "@next/font": "13.1.6",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@architecture-it/eslint-config-andreani": "1.4.0",
    "@architecture-it/stylesystem": "^2.13.0",
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-brands-svg-icons": "6.2.1",
    "@fortawesome/pro-light-svg-icons": "6.2.1",
    "@fortawesome/pro-regular-svg-icons": "6.2.1",
    "@fortawesome/pro-solid-svg-icons": "6.2.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@mui/material": "5.11.6",
    "@next/bundle-analyzer": "13.1.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "sass": "1.58.0",
    "stylelint": "14.5.1",
    "stylelint-config-standard-scss": "3.0.0",
    "typescript": "4.9.5"
  }
}
